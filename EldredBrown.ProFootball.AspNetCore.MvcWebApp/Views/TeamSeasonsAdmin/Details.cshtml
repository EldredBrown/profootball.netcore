@model TeamSeasonsDetailsViewModel

@{
    ViewData["Title"] = "Team Season";
}

<h1>@ViewData["Title"]</h1>

<h4>Details</h4>
<hr />
<div>
    <dl class="row">
        <dt class="col-sm-2">
            <label asp-for="TeamSeason.TeamName"></label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TeamSeason.TeamName)
        </dd>
        <dt class="col-sm-2">
            <label asp-for="TeamSeason.SeasonYear"></label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TeamSeason.SeasonYear)
        </dd>
        <dt class="col-sm-2">
            <label asp-for="TeamSeason.LeagueName"></label>"
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TeamSeason.LeagueName)
        </dd>
    </dl>
</div>

<div>
    <p>
        <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.TeamSeason.ID">Edit</a>
    </p>
    <p>
        <a asp-action="Index">Back to List</a>
    </p>
</div>

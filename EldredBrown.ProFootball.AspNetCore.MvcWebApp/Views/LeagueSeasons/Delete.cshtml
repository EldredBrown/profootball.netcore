@model LeagueSeason

@{
    ViewData["Title"] = "Delete League Season";
}

<h1>League Season</h1>

<h4>Delete</h4>
<hr />
<div>
    <h3>Are you sure you want to delete this?</h3>
    <dl class="row">
        <dt class="col-sm-2">
            <label asp-for="LeagueName"></label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LeagueName)
        </dd>
        <dt class="col-sm-2">
            <label asp-for="SeasonYear"></label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SeasonYear)
        </dd>
        <dt class="col-sm-2">
            <label asp-for="TotalGames"></label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TotalGames)
        </dd>
        <dt class="col-sm-2">
            <label asp-for="TotalPoints"></label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TotalPoints)
        </dd>
        <dt class="col-sm-2">
            <label asp-for="AveragePoints"></label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AveragePoints)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="ID" />
        <p>
            <button class="btn btn-danger" type="submit">Delete</button>
        </p>
        <p>
            <a asp-action="Index">Back to List</a>
        </p>
    </form>
</div>
